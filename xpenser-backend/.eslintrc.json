{
  "root": true,
  "extends": ["eslint:recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "prettier/prettier": "warn",
    "no-useless-escape": 0,
    "no-unused-vars": "warn"
  },
  "ignorePatterns": ["src/**/*.test.ts", "src/frontend/generated/*"]
}
